global _start
#include <stdio.km>
#include <string.km>
#include <math.km>

fn _start() : byte {
	let name : char[512]
	print("What is your name: ")
	readln(name, 512)
	for i = 0 -> 8 ^ 1 {
		if streq(name, "test") print("[TEST] ")
		printi(i + 1)
		print(": Hello, ")
		print(name)
		print("!\n")
	}
	return 0
}